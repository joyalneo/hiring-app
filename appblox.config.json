    "https": "https://github.com/joyalneo/hiring-app",
    "ssh": "git@github.com:joyalneo/hiring-app.git"
  },
  "bloxPrefix": "ha",
  "dependencies": {
    "container": {
      "directory": "view/container/_ha_container",
      "meta": {
        "name": "container",
        "type": "ui-container",
        "source": {
          "https": "https://github.com/joyalneo/_ha_container",
          "ssh": "git@github.com:joyalneo/_ha_container.git"
        },
        "language": "nodejs",
        "start": "npx webpack-dev-server",
        "build": "npm run build",
        "postPull": "npm i"
      }
    },
    "candidate_listing": {
